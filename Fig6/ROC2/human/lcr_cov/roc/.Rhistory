# Load necessary libraries
library(ggplot2)
library(dplyr)
# Set working directory (modify as needed)
setwd("/home/anirjit/ANIRJIT/ROC2/human/lcr_cov/roc")
# Read the data
df <- read.delim("all.tsv", header = TRUE, sep = "\t")
# Ensure Cover_percent is numeric
df$Cover_percent <- as.numeric(df$Cover_percent)
# Define Cover_percent categories
df$Cover_Category <- cut(df$Cover_percent,
breaks = c(-Inf, 5, 10, 15, 20, Inf),
labels = c("0-5", "6-10", "11-15", "16-20", "20+"),
right = TRUE)
# Convert to factor to ensure proper ordering
df$Cover_Category <- factor(df$Cover_Category, levels = c("0-5", "6-10", "11-15", "16-20", "20+"))
# Define distinct colors for the tools
tool_colors <- c("red", "blue", "green", "purple", "orange", "lightblue", "magenta",
"yellow", "brown", "pink", "gray", "black", "darkgreen")
# Plot line graph for Cover_Category vs TPR
jpeg("Cover_percent_vs_TPR.jpg", width = 5000, height = 4000, res = 750)
ggplot(df, aes(x = Cover_Category, y = TPR, color = Tool, group = Tool)) +
geom_line(size = 1) +
geom_point(size = 2) +
scale_color_manual(values = tool_colors) +
labs(title = "Cover Percent vs TPR for Different Tools",
x = "Cover Percent Categories",
y = "True Positive Rate (TPR)",
color = "Tool") +
theme_minimal() +
theme(axis.text.x = element_text(angle = 45, hjust = 1),
panel.grid = element_blank(), # Removes the background grid
axis.line = element_line(color = "black") # Adds x and y axis lines
)
dev.off()
