# Load necessary libraries
library(ggplot2)
# Read the data
df <- read.delim("all.tsv", header = TRUE, sep = "\t")
# Define distinct colors for tools
tool_colors <- c("red", "blue", "green", "purple", "orange", "lightblue", "magenta",
"yellow", "brown", "pink", "gray", "black", "darkgreen")
# Save the plot
#jpeg("LCR_TPR_plot.jpg", width = 5000, height = 4000, res = 500)
# Create the line plot
ggplot(df, aes(x = LCR_num, y = TPR, color = Tool, group = Tool)) +
geom_line(size = 1) +
geom_point(size = 2) +
scale_color_manual(values = tool_colors) +
labs(title = "LCR Number vs. TPR for Different Tools",
x = "LCR Number",
y = "True Positive Rate (TPR)",
color = "Tool") +
theme_minimal() +
theme(panel.grid = element_blank(), # Removes the background grid
axis.line = element_line(color = "black")) # Adds x and y axis lines
